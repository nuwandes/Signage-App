                    android:id="@+id/txtSeventhEstateDestinationEstate_inter_estate_daily_summary"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@drawable/custom_ll_border"
                    android:gravity="center"
                    android:text=""
                    android:textColor="@color/app_text_color"
                    android:textSize="@dimen/bought_leaf_line_report_text_size" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginHorizontal="@dimen/bought_leaf_line_report_layout_horizontal_margin"
            android:layout_weight="1"
            android:background="@drawable/report_layout_background"
            android:orientation="vertical"
            android:paddingHorizontal="@dimen/activity_report_layout_padding"
            android:weightSum="10">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2.5"
                android:gravity="center"
                android:text="@string/confirmed_received_quantities_at_destination_kg"
                android:textColor="@color/app_text_color"
                android:textSize="@dimen/activity_report_headers_text_size"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="7"
                android:gravity="center_vertical"
                android:orientation="vertical"
                android:weightSum="7.7"
                android:layout_marginHorizontal="5dp">

                <TextView
                    android:id="@+id/txtFirstConfirmedReceivedQuantity_inter_estate_daily_summary"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@drawable/custom_ll_border"
                    android:gravity="center"
                    android:text=""
                    android:textColor="@color/app_text_color"
                    android:textSize="@dimen/bought_leaf_line_report_text_size" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.1"
                    />
                <TextView
                    android:id="@+id/txtSecondConfirmedReceivedQuantity_inter_estate_daily_summary"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
